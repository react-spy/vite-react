{
  "name": "vite-react",
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://139.224.54.218:3000/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "continue": true
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
